{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "no-loops",
    "prettier"
  ],
  "rules": {
    "no-console": 1,
    "no-loops/no-loops": 2,
    "prettier/prettier": 2
  }
}
